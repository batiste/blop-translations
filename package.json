{
  "name": "blop-translations",
  "version": "1.0.6",
  "description": "Translating Angular translation file made easy",
  "main": "src/server.blop",
  "bin": {
    "translations": "./index.js"
  },
  "scripts": {
    "start": "RUN=true npm run server",
    "server": "npm run build:server && node dist/server.js",
    "build:server": "node_modules/webpack/bin/webpack.js --config webpack.server.js",
    "build:client": "node_modules/webpack/bin/webpack.js --config webpack.client.js",
    "build": "npm run build:server && npm run build:client"
  },
  "author": "Batiste Bieler",
  "license": "ISC",
  "devDependencies": {},
  "dependencies": {
    "blop-language": "^0.9.11",
    "commander": "^3.0.2",
    "css-loader": "^3.2.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.0",
    "null-loader": "^3.0.0",
    "snabbdom-to-html": "^5.1.1",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-node-externals": "^1.7.2"
  }
}
